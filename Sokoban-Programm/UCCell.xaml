<UserControl x:Name="wndCell" x:Class="Sokoban_Programm.UCCell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Sokoban_Programm"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100">
    <Grid x:Name="grdCell" Width="100" Height="100">

        <Image x:Name="ImgWall" Source="/wall.png" Visibility="Hidden"></Image>
        <Image x:Name="ImgTarget" Source="/target.png" Visibility="Hidden"></Image>
        <Image x:Name="ImgCrate" Source="/crate.png" Visibility="Hidden"></Image>
        <Image x:Name="ImgCoin" Source="/coin.png" Visibility="Hidden"></Image>
        <Image x:Name="ImgGround" Source="/ground_05.png"></Image>
        <Image x:Name="ImgPlayer_Down" Source="/player_down.png" Visibility="Hidden"></Image>
        <Image x:Name="ImgPlayer_Left" Source="/player_left.png" Visibility="Hidden"></Image>
        <Image x:Name="ImgPlayerRight" Source="/player_right.png" Visibility="Hidden"></Image>
        <Image x:Name="ImgPLayer_Up" Source="/player_up.png" Visibility="Hidden"></Image>
        <Image x:Name="ImgCrateTarget" Source="/TargetCrate.png" Visibility="Hidden"/>

    </Grid>
</UserControl>
